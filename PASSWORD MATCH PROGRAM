#include<stdio.h>
#include<string.h>
char str1[10],str2[10];
int main()
{
    printf("Enter a password\n");
    fgets(str1,sizeof(str1),stdin);
printf("Reenter a password\n");
fgets(str2,sizeof(str2),stdin);
if(strcmp(str1,str2)==0)
printf("password match");
else
printf("password mismatch");

return 0;
}
