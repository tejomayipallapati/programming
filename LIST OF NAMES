#include<stdio.h>
#include<string.h>
void main()
{
    char a[10][10];
    int n;
    printf("Enter no.of names:");
    scanf("%d",&n);
    for(int i=0;i<n;i++)
    {
        scanf("\n%s",a[i]);
    }
    printf("List of names is:\n");
    for( int i=0;i<n;i++)
    {
        printf("name%d is :%s\n",i,a[i]);
    }
}
